{
  "name": "aha-exam-backend",
  "scripts": {
    "lintfix": "eslint . --ext .ts,.js,.mjs --fix",
    "lint": "eslint .",
    "ts-build": "node swagger.mjs && npx tsc -p .",
    "ts-start": "npx ts-node -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/app.ts",
    "ts-build-dev": "npx cross-env NODE_ENV=dev node swagger.mjs && npx tsc -p .",
    "dev": "npx cross-env NODE_ENV=dev node ./dist/app.js",
    "start": "npx node ./dist/app.js",
    "start-gendoc": "node swagger.mjs"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~3.2.7",
    "dotenv": "^16.3.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-openid-connect": "^2.16.0",
    "http-errors": "~1.8.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.4.2",
    "module-alias": "^2.2.3",
    "morgan": "~1.10.0",
    "pg": "^8.11.1",
    "secret": "^1.2.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/pg-promise": "^5.4.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "swagger-autogen": "^2.23.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "@model": "dist/model",
    "@routes": "dist/routes",
    "@repo": "dist/repo",
    "@auth0": "dist/repo/auth0",
    "@db": "dist/repo/database",
    "@utils": "dist/utils",
    "@api": "dist/routes/api",
    "@mw": "dist/middleware"
  }
